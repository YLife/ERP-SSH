<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yl.entity">
	<class name="Client">
		<id name="clientId">
			<generator class="native"></generator>
		</id>
		<property name="clientName"></property>
		<property name="clientComp"></property>
		<property name="clientPhone"></property>
		<property name="clientAdd"></property>
		<property name="clientBack"></property>
		<property name="clientDate"></property>
	</class>
</hibernate-mapping>
