<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.yl.entity">
	<class name="Mess">
		<id name="messId">
			<generator class="native"></generator>
		</id>
		<property name="messTitle"></property>
		<property name="messContent"></property>
		<property name="sendName"></property>
		<property name="getName"></property>
		<property name="sendDate"></property>
		<property name="getDate"></property>
	</class>
</hibernate-mapping>
